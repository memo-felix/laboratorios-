var url = "http://uami-cgma.esy.es/prueba/prueba.txt";
var json, nombre;
var arreglo = [];

var xhr = Ti.Network.createHTTPClient({
    onload: function() {
        json = JSON.parse(this.responseText);
        //var pjson =JSON.stringify(json);
        
     
             nombre = json.Fugitivos[0].nombre;
             arreglo.push(nombre);
             	
        
}
});
xhr.open('GET', url);
xhr.send();
function prueba() {

alert(arreglo);
}
$.index.open();
